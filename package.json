    "@types/node": "^14.11.6",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-plugin-sort-class-members": "^1.8.0",
    "mocha": "^8.1.3",
    "ncp": "^2.0.0",
    "npm-check": "^5.9.2",
