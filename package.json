{
  "name": "celes",
  "version": "0.1.0",
  "description": "Celes is an open source file scraper that obtains and stores game achievements, whatever the origin.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncp src/types dist/types",
    "build:clean": "rimraf dist && tsc && ncp src/types dist/types",
    "check-dependencies": "npm-check",
    "check-dependencies:update": "npm-check --update",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "mocha -r ts-node/register 'tests/**/*.ts' --timeout 30000",
    "test:coverage": "nyc mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "contributors": [
