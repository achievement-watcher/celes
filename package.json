    "@types/fs-ext": "^2.0.0",
    "@types/ini": "^1.3.30",
    "@types/lodash.omit": "^4.5.6",
    "@types/normalize-path": "^3.0.0",
    "fast-glob": "^3.2.4",
    "fs-ext": "^2.0.0",
    "got": "^11.7.0",
    "ini": "^1.3.5",
    "lodash.omit": "^4.5.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "normalize-path": "^3.0.0",
